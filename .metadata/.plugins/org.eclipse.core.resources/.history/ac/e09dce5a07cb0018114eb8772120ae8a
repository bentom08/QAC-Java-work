
public class Runner {

	public static void main(String[] args) {
		System.out.println(numbersAndBoolean(5, 4, false));
	}
	
	public static int numbersAndBoolean(int a, int b, boolean bool) {
		if (bool) {
			return a+b;
		} else {
			return a*b;
		}
	}
	
	public static void flowchart(int a) {
		
	}
}
