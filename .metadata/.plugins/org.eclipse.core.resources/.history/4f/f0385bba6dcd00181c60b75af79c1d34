package com.qa.Goldilocks;

public class Runner {

	public static void main(String[] args) {
		Table myTable = new Table(100, 80);
		
		myTable.addSeat(new Seat(30, 50, 100));
		myTable.addSeat(new Seat(130, 60, 100));
		myTable.addSeat(new Seat(90, 60, 5));
		myTable.addSeat(new Seat(150, 85, 5));
		myTable.addSeat(new Seat(120, 70, 5));
		myTable.addSeat(new Seat(200, 200, 5));
		myTable.addSeat(new Seat(110, 100, 5));
		
		myTable.whichSeats();
	}
}
