package com.qa.Factorials;

public class Runner {

	public static void main(String[] args) {

	}
	
	public static String reverseFactorial(int result) {
		int n = 1;
		int factorial = result;
		
		while (result != 1) {
			result /= n;
			n++;
			if (n == 0) {
				return "NONE";
			}
		}
		
		return (n - 1) + "!" + " = " + factorial;
	}
}
