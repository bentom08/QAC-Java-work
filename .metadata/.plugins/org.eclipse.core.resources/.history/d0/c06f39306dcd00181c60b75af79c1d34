package com.qa.Goldilocks;

public class Seat {

	private static int ID = 1;
	private int weight;
	private int temp;
	private int seatNo;
	
	public Seat(int weight, int temp) {
		this.seatNo = ID;
		this.weight = weight;
		this.temp = temp;
		
		ID++;
	}
	
	public int getWeight() {
		return this.weight;
	}
	
	public int getTemp() {
		return this.temp;
	}
	
	public int getSeatNo() {
		return this.seatNo;
	}
}
