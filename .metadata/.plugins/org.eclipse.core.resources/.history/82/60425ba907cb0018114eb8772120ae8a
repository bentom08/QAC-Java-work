
public class Runner {

	public static void main(String[] args) {
		System.out.println(numbersAndBoolean(5, 4, false));
	}
	
	public static int numbersAndBoolean(int a, int b, boolean bool) {
		if (bool) {
			return a+b;
		} else {
			return a*b;
		}
	}
	
	public static void flowchart(int a) {
		if (a > 2000) {
			System.out.println("A");
			if (a > 6000) {
				System.out.println("C");
			} else {
				System.out.println("B");
				if (a > 4000) {
					System.out.println("D");
				} else {
					System.out.println("E");
				}
			}
		} else {
			System.out.println(1);
		}
	}
}
