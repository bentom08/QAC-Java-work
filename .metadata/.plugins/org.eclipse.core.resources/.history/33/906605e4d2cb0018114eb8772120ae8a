package com.qa.CalaculatorLambdas;

public class Runner {

	MathOperation addition = (x, y) -> x + y;
	
	MathOperation subtraction = (x, y) -> x - y;
	
	MathOperation multiplication = (x, y) -> x * y;
	
	MathOperation division = (x, y) -> x / y;
	
	public static void main(String[] args) {
		calculator(10, 5, addition);
	}
	
	public static int calculator(int a, int b, MathOperation operator) {
		return operator.operation(a, b);
	}
}
