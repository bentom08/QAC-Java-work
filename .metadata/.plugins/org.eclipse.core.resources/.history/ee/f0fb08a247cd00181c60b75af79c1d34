package com.qa.Library;

public class Person {

	private String name;
	private int age;
	private String phoneNo;
	
	public Person(String name, int age, String phoneNo) {
		this.name = name;
		this.age = age;
		this.phoneNo = phoneNo;
	}
	
	public String toString() {
		return "Name: " + this.name + "\nAge: " + this.age + "\nPhone Number: " + this.phoneNo;
	}
}
