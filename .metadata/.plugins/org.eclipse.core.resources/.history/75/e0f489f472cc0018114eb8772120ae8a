package com.qa.GarageClass;

import java.util.ArrayList;
import java.util.List;

public class Garage {

	List<Vehicle> myGarage = new ArrayList<>();
	
	public Garage(List<Vehicle> myGarage) {
		this.myGarage = myGarage;
	}
	
	public void storeVehicle(Vehicle myVehicle) {
		myGarage.add(myVehicle);
	}
}
