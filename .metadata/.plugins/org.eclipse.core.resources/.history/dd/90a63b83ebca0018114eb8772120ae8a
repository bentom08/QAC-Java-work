
public class Runner {

	public static void main(String[] args) {
		
	}
	
	public static int adder(int a, int b) {
		return a+b;
	}
	
	public static int subtractor(int a, int b) {
		return a-b;
	}
	
	public static int multiplier(int a, int b) {
		return a*b;
	}
	
	public static int divider(int a, int b) {
		return a/b;
	}
}
