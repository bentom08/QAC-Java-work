
public class Runner {

	public static void main(String[] args) {
		System.out.println(reverse(""));
	}
	
	public static String reverse(String myString) {
		String reversed = "";
		for (int i = myString.length() - 1; i >= 0; i--) {
			reversed += String.valueOf(myString.charAt(i));
		}
		
		return reversed;
	}
}
